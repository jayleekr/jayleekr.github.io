{
  "name": "agreeable-ablation",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "type-check": "astro check",
    "lint": "eslint src --ext .ts,.astro",
    "format": "prettier --write src/**/*.{ts,astro,md}",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:lighthouse": "npx lhci autorun",
    "test:percy": "percy exec -- npm run test:e2e",
    "test:all": "npm run test && npm run test:e2e && npm run test:lighthouse"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "astro": "^5.11.0",
    "astro-i18next": "^1.0.0-beta.21",
    "i18next": "^25.3.2",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^6.0.2",
    "@axe-core/playwright": "^4.10.2",
    "@percy/cli": "^1.31.0",
    "@percy/playwright": "^1.0.8",
    "@playwright/test": "^1.54.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-formatter-compact": "^8.40.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.7.1",
    "lighthouse-ci": "^1.13.1",
    "playwright": "^1.54.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
